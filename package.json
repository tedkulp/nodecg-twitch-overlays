{
  "name": "twitch-viewers",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch-dashboard": "node_modules/.bin/parcel watch src/dashboard/*.html --no-hmr --no-autoinstall --out-dir dashboard --public-url ./",
    "watch-graphics": "node_modules/.bin/parcel watch src/graphics/*.html --no-hmr --no-autoinstall --out-dir graphics --public-url ./",
    "watch-all": "concurrently \"npm run watch-dashboard\" \"npm run watch-graphics\""
  },
  "author": "",
  "license": "ISC",
  "browserslist": "last 2 chrome versions",
  "nodecg": {
    "compatibleRange": ">=0.9",
    "dashboardPanels": [
      {
        "name": "Events",
        "title": "Latest Channel Events",
        "file": "events.html",
        "headerColor": "#6441A4",
        "width": 2
      },
      {
        "name": "Goals",
        "title": "Current Stream Goal",
        "file": "goals.html",
        "headerColor": "#6441A4",
        "width": 3
      }
    ],
    "graphics": [
      {
        "file": "viewers.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "notifications.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "goals.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "chat.html",
        "width": 328,
        "height": 793
      }
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "parcel-bundler": "^1.12.0",
    "sass": "^1.17.2"
  },
  "dependencies": {
    "@date-io/moment": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "color-hash": "^1.0.3",
    "deasync": "^0.1.16",
    "gsap": "^2.1.2",
    "jss": "^10.0.0-alpha.3",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "material-ui-pickers": "^2.2.4",
    "moment": "^2.24.0",
    "pixi.js": "^5.0.0-rc.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-spring": "^8.0.13",
    "twitch-emoticons": "^2.1.1",
    "use-nodecg": "^0.4.1-1",
    "webfontloader": "^1.6.28"
  }
}
